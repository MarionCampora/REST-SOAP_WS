<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      
      <service name="Wcf_WS.Service1" behaviorConfiguration="VelibBehavior">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/VelibService"/>
          </baseAddresses>
        </host>

        <endpoint address="" binding="wsDualHttpBinding" contract="Wcf_WS.IService1"/>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
      </service>
    </services>

    <behaviors>
      <serviceBehaviors>
        <behavior name="VelibBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="true "/>
        </behavior>
      </serviceBehaviors>
    </behaviors>

  </system.serviceModel>
</configuration>



